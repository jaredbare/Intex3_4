@model Burialmain
<!-- Modal -->
<div class="modal fade" id="AddBurial" aria-labelledby="AddBurialLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="addDetailsLabel">Add Burial Details</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form asp-action="Create" asp-controller="Home" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Squarenorthsouth" class="control-label"></label>
                        <input asp-for="Squarenorthsouth" class="form-conrol" />
                        <span asp-validation-for="Squarenorthsouth" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Headdirection" class="control-label"></label>
                        <input asp-for="Headdirection" class="form-conrol" />
                        <span asp-validation-for="Headdirection" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Sex" class="control-label"></label>
                        <input asp-for="Sex" class="form-conrol" />
                        <span asp-validation-for="Sex" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Northsouth" class="control-label"></label>
                        <input asp-for="Northsouth" class="form-conrol" />
                        <span asp-validation-for="Northsouth" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Depth" class="control-label"></label>
                        <input asp-for="Depth" class="form-conrol" />
                        <span asp-validation-for="Depth" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Eastwest" class="control-label"></label>
                        <input asp-for="Eastwest" class="form-conrol" />
                        <span asp-validation-for="Eastwest" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Adultsubadult" class="control-label"></label>
                        <input asp-for="Adultsubadult" class="form-conrol" />
                        <span asp-validation-for="Adultsubadult" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Facebundles" class="control-label"></label>
                        <input asp-for="Facebundles" class="form-conrol" />
                        <span asp-validation-for="Facebundles" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Southtohead" class="control-label"></label>
                        <input asp-for="Southtohead" class="form-conrol" />
                        <span asp-validation-for="Southtohead" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Preservation" class="control-label"></label>
                        <input asp-for="Preservation" class="form-conrol" />
                        <span asp-validation-for="Preservation" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Fieldbookpage" class="control-label"></label>
                        <input asp-for="Fieldbookpage" class="form-conrol" />
                        <span asp-validation-for="Squareeastwest" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Squareeastwest" class="control-label"></label>
                        <input asp-for="Squareeastwest" class="form-conrol" />
                        <span asp-validation-for="Squareeastwest" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Goods" class="control-label"></label>
                        <input asp-for="Goods" class="form-conrol" />
                        <span asp-validation-for="Goods" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Text" class="control-label"></label>
                        <input asp-for="Text" class="form-conrol" />
                        <span asp-validation-for="Text" class="text-danger"></span>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="submit" class="btn-primary">Save</button>
                    </div>

                </form>



            </div>
        </div>
    </div>
</div>
